{
  "name": "cs-nuxt-prismic",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "storybook": "nuxt storybook",
    "build-storybook": "nuxt storybook build",
    "slicemachine": "start-slicemachine --port 9999",
    "bundle": "node ./node_modules/sm-commons/scripts/bundle"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/prismic": "^1.2.4",
    "@prismicio/vue": "^2.0.7",
    "core-js": "^3.6.5",
    "global": "^4.4.0",
    "nuxt": "^2.14.6",
    "nuxt-sm": "^0.0.6",
    "prismic-cli": "3.8.7",
    "prismic-javascript": "^3.0.2",
    "slice-machine-ui": "0.0.43",
    "sm-commons": "^0.0.23",
    "vue-essential-slices": "^0.3.0",
    "vue-slicezone": "^0.0.29"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/google-fonts": "^1.2.0",
    "@nuxtjs/storybook": "^3.3.1",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "eslint": "^7.10.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^5.0.9",
    "lint-staged": "^10.4.0",
    "modern-normalize": "^1.0.0",
    "node-sass": "^5.0.0",
    "prettier": "^2.1.2",
    "sass": "1.32.7",
    "sass-loader": "11.0.1",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "3.19.0"
  }
}
